{
    "acct_gather_file": {},
    "acct_gather_section": {},
    "accounting_storage_section": {
        "AccountingStorageEnforce": "limits,associations,qos",
        "AccountingStorageHost": "",
        "AccountingStorageTRES": "gres/gpu",
        "AccountingStorageType": "accounting_storage/slurmdbd",
        "AccountingStorageUser": "slurm"
    },
    "AssocBackup": {
        "cmd": "/usr/bin/sacctmgr dump",
        "copies" : "7"
    },
    "AuthType": "auth/munge",
    "backup_db_dir": "/var/backups",
    "backup_dir": "$(scl.slurm[backup_db_dir])/slurm",
    "backup_schedule": [ "Hr00.Min00_05" ],
    "backup_config": {
        "cmd": "/usr/bin/mysqldump",
        "copies" : "7",
        "host": "localhost",
        "options": "--opt --flush-logs --quote-names --all-databases",
        "password": "",
        "user": ""
    },
    "ClusterName": "",
    "ControlMachine": "",
    "cgroup_allowed_file": [
        "/dev/null",
        "/dev/urandom",
        "/dev/zero",
        "/dev/sda*",
        "/dev/cpu/*/*",
        "/dev/pts/*"
    ],
    "cgroup_file": [
        "CgroupAutomount=yes",
        "ConstrainCores=yes",
        "ConstrainDevices=yes",
        "## This set the memory hardlimt of a job to % of node memory",
        "ConstrainRAMSpace=yes",
        "ConstrainSwapSpace=yes",
        "AllowedRAMSpace=100.0",
        "AllowedSwapSpace=0.0"
    ],
    "copy_dirs": [],
    "DebugFlags": [
        "priority",
        "switch",
        "backfill"
    ],
    "disable_services": [
        "slurmd",
        "slurmdbd",
        "slurmctld",
        "slurmrestd"
    ],
    "Epilog": "$(slurm.opt_dir)/epilog/slurmd/*",
    "gres_file": [],
    "GresTypes": "gpu",
    "health_section": {},
    "InactiveLimit": "0",
    "interactive_step_options": {
        "options": "\"--interactive --preserve-env --pty $SHELL\""
    },
    "include_files": [],
    "JobContainerType": "job_container/none",
    "job_container_tmpfs": {
        "AutoBasePath": "true",
        "lines": [
            "BasePath=/data/slurm Dirs=/tmp,/var/tmp,/dev/shm,/scratch shared=true"
        ]
    },
    "KillWait": "30",
    "launch_parameters": {
        "options": "use_interactive_step"
    },
    "licenses_section": [],
    "logrotate_schedule": [ "Hr00.Min00_05" ],
    "MessageTimeout": "10",
    "MinJobAge": "300",
    "MpiDefault": "none",
    "no_default_section": {},
    "nodes_section": [],
    "packages_client": { },
    "packages_server": { },
    "packages_submit": { },
    "partition_section": [],
    "pid_dir": "/var/run/slurm-llnl",
    "powersave_section": {
        "ResumeTimeout": "60"
    },
    "private_tmpdir_section": [],
    "priority_section": {
        "PriorityDecayHalfLife": "7-00",
        "PriorityFlags": "FAIR_TREE,MAX_TRES",
        "PriorityWeightAge": "100000",
        "PriorityWeightFairshare": "1000000",
        "PriorityWeightPartition": "1000000",
        "PriorityMaxAge": "7-00",
        "PriorityType": "priority/multifactor"
    },
    "Prolog": "$(slurm.opt_dir)/prolog/slurmd/*",
    "PrologEpilogTimeout": "3600",
    "PrologFlags": "x11",
    "prolog_section": {},
    "PrologSlurmctld":"$(slurm.opt_dir)/prolog/slurmctld/check",
    "purge_section": {
        "PurgeJobAfter": "12month",
        "PurgeStepAfter": "1month"
    },
    "scheduling_section": {
        "comment": [
            "SCHEDULING",
            "Toplogy settings max_switch_wait=7200, wait 2 hours for optimal job switch configuration",
            "*_max_job_start: limits the amount of started jobs per cycle",
            "bf_continue: backfilling is not blocking",
            "bf_window: look 24 hours into the future",
            "kill_invalid_depend: cancel jobs that can never be satisfied"
        ],
        "parameters": [
            "bf_continue",
            "bf_interval=30",
            "bf_max_job_start=10",
            "bf_max_job_test=100",
            "bf_max_job_user=10",
            "bf_resolution=300",
            "bf_window=1440",
            "bf_yield_interval=1000000",
            "kill_invalid_depend",
            "nohold_on_prolog_fail",
            "max_rpc_cnt=150",
            "max_switch_wait=7200",
            "sched_max_job_start=20"
        ],
        "settings": {
            "SchedulerType": "sched/backfill",
            "SelectType": "select/cons_tres",
            "SelectTypeParameters": "CR_Core_Memory"
        }
    },
    "slurm_user": {
        "uid": "555",
        "user": "slurm",
        "gid": "555",
        "group": "slurm"
    },
    "SlurmctldDebug": "info",
    "SlurmctldPort": "6817",
    "SlurmctldTimeout": "120",
    "slurmctld_settings": {},
    "SlurmdDebug": "info",
    "SlurmdPort": "6818",
    "SlurmdTimeout": "300",
    "slurmdbd_service": {
        "options": "",
        "pid_file": "/run/slurmdbd.pid",
        "type": "forking"
    },
    "slurmctld_service": {
        "options": "",
        "pid_file": "/run/slurmctld.pid",
        "type": "forking"
    },
    "slurmd_service": {
        "options": "-D",
        "pid_file": "/run/slurmd.pid",
        "type": "simple"
    },
    "slurmrestd_service": {
        "options": "127.0.0.1:6666",
        "type": "simple"
    },
    "slurmrestd_user": {
        "uid": "553",
        "user": "slurmrestd",
        "gid": "553",
        "group": "slurmrestd"
    },
    "spank_plugins": {
        "required": {
            "${slurm.dir}/lib/private-tmpdir.so": " base=/scratch/slurm mount=/tmp mount=/var/tmp mount=/scratch base=/dev/shm/slurm mount=/dev/shm"
        }
    },
    "StoragePass": "not_set",
    "taskplugin_section": {
        "TaskPlugin": "task/affinity,task/cgroup"
    },
    "tmpfs": "/scratch",
    "topology_file": [],
    "UnkillableStepTimeout": "60",
    "WaitTime": "0"
}
