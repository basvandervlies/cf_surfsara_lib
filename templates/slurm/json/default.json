{
    "AccountingStorageHost": "",
    "AccountingStorageTRES": "gres/gpu",
    "AuthType": "auth/munge",
    "backup_dir": "$(sara_data.slurm[opt_dir])/backup",
    "backup_schedule": [ "Hr00.Min00_05" ],
    "backup_config": {
        "cmd": "/usr/bin/mysqldump",
        "copies" : "7",
        "host": "localhost",
        "options": "--opt --flush-logs --quote-names --all-databases",
        "password": "",
        "user": ""
    },
    "ClusterName": "",
    "ControlMachine": "",
    "cgroup_allowed_file": [
        "/dev/null",
        "/dev/urandom",
        "/dev/zero",
        "/dev/sda*",
        "/dev/cpu/*/*",
        "/dev/pts/*"
    ],
    "cgroup_file": [
        "CgroupAutomount=yes",
        "ConstrainCores=yes",
        "ConstrainDevices=yes",
        "## This set the memory hardlimt of a job to % of node memory",
        "ConstrainRAMSpace=yes",
        "MaxRAMPercent=98.0",
        "## We have to decide if we need TaskAffinity",
        "TaskAffinity=no"
    ],
    "copy_dirs": [],
    "DebugFlags": [
        "priority",
        "switch",
        "backfill"
    ],
    "Epilog": "$(sara_data.slurm[opt_dir])/epilog/slurmd/*",
    "gres_file": [],
    "GresTypes": "gpu",
    "health_section": {},
    "InactiveLimit": "0",
    "include_files": [],
    "KillWait": "30",
    "licenses_section": [],
    "logrotate_schedule": [ "Hr00.Min00_05" ],
    "MessageTimeout": "10",
    "MinJobAge": "300",
    "MpiDefault": "none",
    "no_default_section": {},
    "nodes_section": [],
    "opt_dir": "/opt/slurm",
    "partition_section": [],
    "spank_templates": {},
    "private_tmpdir_section": [],
    "priority_section": {
        "PriorityDecayHalfLife": "7-00",
        "PriorityFlags": "FAIR_TREE",
        "PriorityWeightAge": "100000",
        "PriorityWeightFairshare": "1000000",
        "PriorityMaxAge": "7-00",
        "PriorityType": "priority/multifactor"
    },
    "Prolog": "$(sara_data.slurm[opt_dir])/prolog/slurmd/*",
    "PrologEpilogTimeout": "3600",
    "PrologFlags": "x11",
    "PrologSlurmctld":"$(sara_data.slurm[opt_dir])/prolog/slurmctld/check",
    "ResumeTimeout": "600",
    "scheduling_section": {
        "comment": [
            "SCHEDULING",
            "Toplogy settings max_switch_wait=7200, wait 2 hours for optimal job switch configuration",
            "*_max_job_start: limits the amount of started jobs per cycle",
            "bf_continue: backfilling is not blocking",
            "bf_window: look 24 hours into the future",
            "kill_invalid_depend: cancel jobs that can never be satisfied"
        ],
        "parameters": [
            "nohold_on_prolog_fail",
            "max_switch_wait=7200",
            "bf_max_job_start=10",
            "sched_max_job_start=20",
            "bf_continue",
            "bf_interval=60",
            "bf_max_job_test=500",
            "bf_max_job_user=10",
            "bf_resolution=300",
            "bf_window=1440",
            "kill_invalid_depend",
            "max_rpc_cnt=150"
        ],
        "settings": {
            "SchedulerType": "sched/backfill",
            "SelectType": "select/cons_res",
            "SelectTypeParameters": "CR_Core_Memory",
            "FastSchedule": "1"
        }
    },
    "scripts_dir": "$(sara_data.slurm[opt_dir])/scripts",
    "SlurmctldDebug": "debug3",
    "SlurmctldPort": "6817",
    "SlurmctldTimeout": "120",
    "SlurmdDebug": "debug3",
    "SlurmdPort": "6818",
    "SlurmdTimeout": "300",
    "SlurmUser": "slurm",
    "StoragePass": "not_set",
    "tmpfs": "/scratch",
    "topology_file": [],
    "UnkillableStepTimeout": "60",
    "WaitTime": "0"
}
